{
  "name": "nodex-0",
  "version": "1.0.0",
  "description": "nodex-0 base on nodex-libs",
  "main": "src/app.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=dev nodemon src/app.js",
    "stage": "pm2-runtime start pm2.stage.json",
    "prod": "pm2-runtime start pm2.prod.json",
    "debug:dev": "cross-env NODE_ENV=dev node src/app.js",
    "debug:stage": "cross-env NODE_ENV=stage node src/app.js",
    "debug:prod": "cross-env NODE_ENV=prod node src/app.js"
  },
  "dependencies": {
    "mongodb": "^3.6.2",
    "nodex-libs": "^2.1.4"
  },
  "devDependencies": {
    "cross-env": "^5.2.1",
    "nodemon": "^2.0.2",
    "pm2": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leansocket/nodex-0.git"
  },
  "license": "MIT"
}
